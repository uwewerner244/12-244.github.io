{   "name":         "cv"
  , "title":        "CV, Resume, Portfolio, Skills Site"
  , "description":  "CV, Resume, Portfolio, Skills, Bio, All-In-One Page Site"
  , "version":      "0.1.0"
  , "authors": [
      "tbaltrushaitis@gmail.com"
    ]
  , "license":  "MIT"
  , "private":  false
  , "homepage": "https://github.com/tbaltrushaitis/cv"
  , "main":     ""
  , "keywords": [
      "bio"
    , "cv"
    , "cv-template"
    , "portfolio"
    , "portfolio-website"
    , "resume"
    , "skills"
    , "skill-demo"
    ]
  , "moduleType": [
      "project"
    ]
  , "ignore": [
        "**/.*"
      , "build"
      , "bower_modules"
      , "dist"
      , "docs"
      , "node_modules"
      , "test"
      , "tests"
      , "webroot"
    ]
  , "dependencies": {
        "animate.css":   "~3.5.2"
      , "bootstrap":     "~3.3.7"
      , "font-awesome":  "~4.7.0"
      , "html5shiv":     "~3.7.3"
      , "jquery":        "~2.2.4"
      , "noty":          "~2.4.1"
      , "respond":       "~1.4.2"
      , "requirejs":     "~2.3.3"
      , "waypoints":     "^4.0.1"
      , "wow":           "wow-mit#~1.3.0"
      , "normalize-css": "normalize.css#7.0.0"
    }
  , "devDependencies": {
    }
  , "disabledDependencies": {
      "normalize.css": "~4.2.0"
    }
  , "resolutions": {
      "jquery":   "~2.2.4"
    }
  , "overrides": {
      "bootstrap": {
        "main": [
            "dist/**/*.*"
          , "!**/*.less"
          , "!**/npm.js"
          , "!**/*.min.js"
          , "!*-theme.*"
        ]
      }
    , "font-awesome": {
        "main": [
            "fonts/*"
          , "css/*.css"
          , "!css/*.min.css"
        ]
      }
    , "waypoints": {
        "main": [
          "lib/**"
        ]
      }
    }
  , "group": {
      "effects":    [
          "animate.css"
        , "normalize-css"
        , "waypoints"
        , "wow"
      ]
    , "fonts":    [
        "font-awesome"
      ]
    , "libs": [
          "jquery"
        , "bootstrap"
        , "requirejs"
      ]
    , "plugins": [
          "html5shiv"
        , "respond"
        , "noty"
      ]
    , "front": ["libs", "effects", "fonts", "plugins"]
    }
}
