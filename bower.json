{   "name": "cv"
  , "title": "Best-in-Class modern CV, Resume and Portfolio website"
  , "description": "Best-in-Class modern CV, Resume and Portfolio website template. Easy customizable All-in-One-Page site with configurable builder."
  , "version": "0.2.0"
  , "authors": [
      "tbaltrushaitis@gmail.com"
    ]
  , "license": "MIT"
  , "private": false
  , "homepage": "https://github.com/tbaltrushaitis/cv"
  , "main": ""
  , "keywords": [
      "bio"
      , "cv"
      , "cv-template"
      , "portfolio"
      , "portfolio-website"
      , "resume"
      , "skills"
      , "skill-demo"
    ]
  , "moduleType": [
      "project"
    ]
  , "ignore": [
        "**/.*"
      , "bower_modules"
      , "build"
      , "dist"
      , "docs"
      , "node_modules"
      , "test"
      , "tests"
      , "webroot"
    ]
  , "dependencies": {
        "animate.css":    "^3.7.0"
      , "bootstrap":      "~3.3.7"
      , "bower-jquery-easyPieChart": "^0.0.1"
      , "font-awesome":   "^5.4.2"
      , "fitvids":        "jquery.fitvids#^1.2.0"
      , "html5shiv":      "~3.7.3"
      , "jquery":         "~3.3.1"
      , "jquery-countTo": "countto#^1.2.0"
      , "jquery.inview":  "jquery.inview#^1.1.2"
      , "jquery.stellar": "^0.6.2"
      , "jquery-sticky":  "sticky#1.0.4"
      , "jquery-throttle-debounce": "*"
      , "magnific-popup": "^1.1.0"
      , "modernizr":      "^3.6.0"
      , "noty":           "~2.4.1"
      , "requirejs":      "~2.3.6"
      , "respond":        "~1.4.2"
      , "smoothscroll":   "^0.4.2"
      , "shufflejs":      "^5.2.0"
      , "waypoints":      "^4.0.1"
      , "wow":            "wow-mit#~1.3.0"
    }
  , "devDependencies": {
    }
  , "disabledDependencies": {
        "jquery.shuffle": "#9cf47a1590"
      , "normalize-css":  "^8.0.0"
      , "segoe-fonts":    "^1.0.1"
    }
  , "resolutions": {
    }
  , "overrides": {
      "bootstrap": {
        "main": [
            "dist/**/*.*"
          , "!**/*.less"
          , "!**/npm.js"
          , "!**/*.min.js"
          , "!*-theme.*"
        ]
        , "main-4": [
              "dist/js/bootstrap.js"
            , "dist/css/bootstrap.css"
            , "dist/fonts/*.*"
          ]
      }
    , "font-awesome": {
        "main": [
            "webfonts/*.*"
          , "css/all.css"
        ]
      , "main-off": [
            "web-fonts-with-css/webfonts/*.*"
          , "web-fonts-with-css/css/fontawesome-all.css"
          , "**/css/*.css"
          , "!css/*.min.css"
        ]
      }
    , "smoothscroll": {
        "main": [
          "src/smoothscroll.js"
        ]
      }
    , "jquery.shuffle": {
        "main": [
          "jquery.shuffle.js"
        ]
      }
    , "shufflejs": {
        "main": [
          "dist/shuffle.js"
        ]
      }
    , "waypoints": {
        "main": [
            "lib/jquery.waypoints.js"
          , "lib/shortcuts/inview.js"
        ]
      }
    }
  , "group": {
      "fonts": [
          "font-awesome"
        , "segoe-fonts"
      ]
    , "libs": [
          "jquery"
        , "bootstrap"
        , "requirejs"
      ]
    , "plugins": [
          "bower-jquery-easyPieChart"
        , "html5shiv"
        , "magnific-popup"
        , "modernizr"
        , "fitvids"
        , "jquery-countTo"
        , "jquery.inview"
        , "jquery.stellar"
        , "jquery-sticky"
        , "jquery-throttle-debounce"
        , "shufflejs"
        , "noty"
        , "respond"
      ]
    , "ui": [
        "animate.css"
        , "normalize-css"
        , "smoothscroll"
        , "shufflejs"
        , "waypoints"
        , "wow"
      ]
    , "front": ["fonts", "libs", "plugins", "ui"]
    }
}
